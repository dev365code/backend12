###  실제 운영 DB 설정 ###
db.default.driverClassName=com.mysql.cj.jdbc.Driver
db.default.url=jdbc:mysql://your-rds-endpoint/your-database?serverTimezone=Asia/Seoul
db.default.username=your_db_username
db.default.password=your_db_password

### 개발/로깅용 log4jdbc 설정 ###
db.logging.driverClassName=net.sf.log4jdbc.sql.jdbcapi.DriverSpy
db.logging.url=jdbc:log4jdbc:mysql://localhost:3306/your_local_db?serverTimezone=Asia/Seoul
db.logging.username=your_local_username
db.logging.password=your_local_password